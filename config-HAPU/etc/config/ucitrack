
config network 'network_1'
	list affects 'dhcp'
	list affects 'radvd'
	option init 'network'

config wireless 'wireless_2'
	list affects 'network'

config firewall 'firewall_3'
	list affects 'luci-splash'
	list affects 'qos'
	list affects 'miniupnpd'
	list affects 'sqm'
	option init 'firewall'

config olsr 'olsr_4'
	option init 'olsrd'

config dhcp 'dhcp_5'
	list affects 'odhcpd'
	list affects 'named'
	option init 'dhcpd'

config odhcpd 'odhcpd_6'
	option init 'odhcpd'

config dropbear 'dropbear_7'
	option init 'dropbear'

config httpd 'httpd_8'
	option init 'httpd'

config fstab 'fstab_9'
	option init 'fstab'

config qos 'qos_10'
	option init 'qos'

config system 'system_11'
	list affects 'luci_statistics'
	option init 'led'

config luci_splash 'luci_splash_12'
	option init 'luci_splash'

config upnpd 'upnpd_13'
	option init 'miniupnpd'

config ntpclient 'ntpclient_14'
	option init 'ntpclient'

config samba 'samba_15'
	option init 'samba'

config tinyproxy 'tinyproxy_16'
	option init 'tinyproxy'

config mwan3
	option exec '/etc/init.d/mwan3 reload'

config sqm
	option init 'sqm'

